[Unit]
Description=NSQ admin
After=network.target
After=runlevel2.target
After=runlevel3.target
After=runlevel4.target
After=runlevel5.target
[Service]
EnvironmentFile=<%= @basic[:libhome] %>/env.sh
ExecStart=<%= @basic[:home] %>/nsqd/bin/nsqadmin --lookupd-http-address=$MEGAM_NSLOOKUPD_IP:4161
KillMode=process
